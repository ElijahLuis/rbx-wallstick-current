}

export type ManualHumanoid = {
	Died: ScriptSignal,
	Running: ScriptSignal,
	Jumping: ScriptSignal,
	Climbing: ScriptSignal,
	GettingUp: ScriptSignal,
	FreeFalling: ScriptSignal,
	FallingDown: ScriptSignal,
